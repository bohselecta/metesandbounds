{
  "name": "parcel-link-finder",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.6.1",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "smoke:live": "node scripts/smoke.mjs",
    "build:cloudflare": "pnpm install && pnpm exec turbo run build --filter=@parcel-link/data && cd apps/web && pnpm run build:cloudflare",
    "deploy:cloudflare": "pnpm run build:cloudflare && cd apps/web && pnpm run deploy"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^22.13.4",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "turbo": "^2.5.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^3.0.8"
  }
}
